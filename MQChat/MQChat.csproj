    <UseWPF>true</UseWPF>
    <ApplicationIcon>icon.ico</ApplicationIcon>
    <Authors>管省超</Authors>
    <Version>0.0.0.1</Version>
    <Copyright>Copyright © GSC 2023</Copyright>
    <RepositoryUrl>https://github.com/GSCmax/MQChat</RepositoryUrl>
